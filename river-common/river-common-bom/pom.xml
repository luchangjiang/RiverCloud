<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>spring-cloud-dependencies-parent</artifactId>
        <groupId>org.springframework.cloud</groupId>
        <version>2.1.3.RELEASE</version>
        <relativePath/>
    </parent>

    <artifactId>river-common-bom</artifactId>
    <packaging>pom</packaging>
    <version>1.0.1</version>
    <description>river 公共版本控制</description>

    <properties>
        <river.version>1.0.1</river.version>
        <jackson.modules>2.9.6</jackson.modules>
        <mybatis-plus.version>3.0.6</mybatis-plus.version>
        <mysql.connector.version>8.0.13</mysql.connector.version>
    </properties>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-core</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-log</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-security</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-data</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-gateway</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-minio</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-common-swagger</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-upms-api</artifactId>
                <version>${river.version}</version>
            </dependency>
            <dependency>
                <groupId>com.cloud.river</groupId>
                <artifactId>river-upms-biz</artifactId>
                <version>${river.version}</version>
            </dependency>
            <!--jackson模块-->
            <dependency>
                <groupId>com.fasterxml.jackson.module</groupId>
                <artifactId>jackson-modules-java8</artifactId>
                <version>${jackson.modules}</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <!--mybatis plus extension,包含了mybatis plus core-->
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-extension</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <!--mybatis-->
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-boot-starter</artifactId>
                <version>${mybatis-plus.version}</version>
            </dependency>
            <!--mysql 驱动-->
            <dependency>
                <groupId>mysql</groupId>
                <artifactId>mysql-connector-java</artifactId>
                <version>${mysql.connector.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>
</project>